#include<stdio.h>
#include<string.h>
int main()
{
 char a[20];
 int b;
 printf("enter the string \n");
 gets(a);

 b=strlen(a);
  printf("string length is %d",b);

  return 0;


}


2->

#include<stdio.h>
#include<string.h>
 int main()
 {
   char a[20],b[20];
   printf("enter the first string \n");
   gets(a);

   printf("enter the second string \n");
   gets(b);

   if(strcmp(a,b)==0)

    printf("string are equal");

   else


    printf("string are not euqal");

   return 0;

 }
 
 3->
#include<stdio.h>
#include<string.h>

int main()
{
    char a[100],b[100],c[100];
    printf("enter the first string:");
    gets(a);
    printf("enter the second string");
    gets(b);
    strcpy(c,a);
    strcpy(a,b);
    strcpy(b,c);

    printf("string after %s",a);
    printf("string after %s",b);

    return 0;

}


 
